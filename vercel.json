{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "allowOrigins": [ "http://this.example.com" ],
    "allowMethods": [ "GET" ]
}
